{
    "name": "ksmz/gplay-encryptedpasswd",
    "description": "Handles the creation of the EncryptedPasswd field, required in Play Store's API calls",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "ksmz",
            "email": "ksmz@amatsuka.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "ksmz\\GPEncryptedPasswd\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8",
        "phpseclib/phpseclib": "^3.0",
        "paragonie/hidden-string": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
